<div class="card mb-2">
    <div class="card-header bg-dark">
        <h5><i class="fas fa-fire"></i> Most Popular</h5> 
    </div>
    <div class="card-body">
        <div class="row">
           <% images.forEach(function(images) { %>
             <div class="col-md-4">
                 <a href="/images/<%= images.uniqueId %>">
                     <img src="/public/upload/<%= images.filename %>" class="w-100 h-100 image-thumbnail" alt="">
                 </a>
             </div>
           <% }); %>
        </div>
    </div>
</div>